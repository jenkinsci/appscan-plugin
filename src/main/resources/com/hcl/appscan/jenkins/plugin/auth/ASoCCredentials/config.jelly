	<f:entry title="${%label.url}" field="url">
    	    <f:textbox placeholder="${%label.credentials.placeholder}" />
    	</f:entry>

    <f:entry title="${%label.id}" field="username">
	    <f:textbox />
	</f:entry>
	
	<f:entry title="${%label.secret}" field="password">
	    <f:password />
	</f:entry>
	
	<f:entry title="${%label.label}" field="description">
		<f:textbox />
	</f:entry>

	<f:entry field="certificates">
	    <f:checkbox title="${%label.certificates}" name="certificates"/>
	</f:entry>
	
</j:jelly>
